<div class="tab-pane fade p-5" id="nav-create" role="tabpanel" aria-labelledby="nav-create-tab">
  <% if projects_drafts.present?  %>
    <h1>Drafts</h1>
  <% end %>
  <% projects_drafts.each do |project_draft| %>
    <%= link_to user_project_path(project_draft) do %>
      <div class="card-proj-dashbord">
        <%= cl_image_tag project_draft.project.photo.key, alt: "alttext", width: 120, height: 120, crop: :thumb %>
        <div class="card-proj-infos w-100">
          <div class="proj-info-text">
            <h2><%= project_draft.project.title %></h2>
          </div>
        </div>
      </div>
    <% end %>
  <% end %>
  <% if projects_published.present? %>
    <h1>Projects you created</h1>
  <% end %>
  <% projects_published.each do |project_published| %>
    <%= link_to user_project_path(project_published) do %>
      <div class="card-proj-dashbord">
        <%= cl_image_tag project_published.project.photo.key, alt: "alttext", width: 120, height: 120, crop: :thumb %>
        <div class="card-proj-infos w-100">
          <div class="proj-info-text">
            <h2><%= project_published.project.title %></h2>
          </div>
        </div>
      </div>
    <% end %>
  <% end %>
</div>
